<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-lazy="false">
    <class name="com.smart.domain.SchoolEvent" table="t_event">
        <id name="eventId" column="event_id">
            <generator class="assigned" />
        </id>
        <property name="schoolCode" column="school_code" />
        <property name="schoolName" column="school_name" />
        <property name="eventText" column="event_text"/>
        <property name="startDate" column="start_date"/>
        <property name="endDate" column="end_date"/>
    </class>
</hibernate-mapping>
